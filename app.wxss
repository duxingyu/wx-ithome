@import './wxParse/wxParse.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.vc::before,
.rc::before {
  display: inline-block;
  margin: 0 10px 14px 0;
  width: 12px;
  height: 12px;
  vertical-align: middle;
}
.vc::before {
  content: url(data:image/png;base64,);
}
.rc::before {
  content: url(data:image/png;base64,);
}
